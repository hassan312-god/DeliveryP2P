# Configuration Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Configuration Google Maps
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Configuration de l'application
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8000

# Configuration de la base de données (si utilisée en local)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=livraisonp2p
DB_USER=postgres
DB_PASSWORD=your_password

# Configuration email (géré par Supabase)
# Les emails sont automatiquement gérés par Supabase Auth
# Pas besoin de configuration SMTP supplémentaire

# Configuration de sécurité
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_key_here

# Configuration Web Push (optionnel)
VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here

# Configuration Supabase Storage
SUPABASE_STORAGE_BUCKET=livraisonp2p

# Configuration des notifications
NOTIFICATION_CHANNELS=realtime,email,webpush

# Configuration de développement
LOG_LEVEL=debug
CACHE_DRIVER=file
SESSION_DRIVER=file 